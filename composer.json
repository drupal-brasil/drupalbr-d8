{
    "type": "project",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "^1.6.4",
        "oomphinc/composer-installers-extender": "^1.1",
        "wikimedia/composer-merge-plugin": "^1.4.1",
        "drupal/panelizer": "^4.1.0",
        "drupal/simple_sitemap": "^2.12.0",
        "drupal/inline_entity_form": "^1.0.0-beta1",
        "drupal/entity_embed": "^1.0.0-beta2",
        "drupal/metatag": "^1.5.0",
        "drupal/image_widget_crop": "^2.1.0",
        "drupal/lightning_workflow": "^2.0.0-rc2",
        "drupal/conditional_message": "^1.0.0-alpha4",
        "drupal/crop": "^2.0.0-rc1",
        "drupal/lightning_media": "^2.3.0",
        "drupal/contact_storage": "^1.0.0-beta9",
        "drupal/embed": "^1.0.0",
        "drupal/lightning_layout": "^1.2.0",
        "drupal/diff": "^1.0.0-rc1",
        "drupal/dropzonejs": "^2.0.0-alpha3",
        "drupal/search_api": "^1.8.0",
        "drupal/lightning_core": "^2.6.0",
        "drupal/jsonapi": "^1.16.0",
        "drupal/google_analytics": "^2.2.0",
        "drupal/media_entity_twitter": "^2.0.0-alpha2",
        "drupal/consumers": "^1.0.0-beta3",
        "drupal/recaptcha": "^2.3.0",
        "drupal/video_embed_field": "^2.0.0",
        "drupal/views_infinite_scroll": "^1.5.0",
        "drupal/panels": "^4.3.0",
        "drupal/entity_block": "^1.0.0-beta1",
        "drupal/admin_toolbar": "^1.24.0",
        "drupal/openapi": "^1.0.0-alpha1",
        "drupal/lightning_api": "^2.3.0",
        "drupal/schemata": "^1.0.0-alpha3",
        "drupal/pathauto": "^1.2.0",
        "drupal/captcha": "^1.0.0-beta1",
        "drupal/ctools": "^3.0.0",
        "drupal/media_entity_instagram": "^2.0.0-alpha1",
        "drupal/simple_oauth": "^3.6.0",
        "drupal/acquia_connector": "^1.14.0",
        "drupal/token": "^1.1.0",
        "drupal/redirect": "^1.2.0",
        "drupal/entity_browser": "^2.0.0-alpha2",
        "drupal/core": "^8.5.3"
    },
    "require-dev": {},
    "extra": {
        "enable-patching": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "merge-plugin": {
            "include": [
                "docroot/modules/custom/*/composer.json"
            ],
            "replace": false,
            "ignore-duplicates": true
        },
        "patches": {}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
